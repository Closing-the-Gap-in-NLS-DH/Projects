{
  "name": "mldh",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "vite build",
    "deploy": "npm run build && gh-pages -b gh-pages -d dist -t true",
    "dev": "vite",
    "format": "prettier --write .",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore --fix . && stylelint --fix src/styles/app.css",
    "serve": "vite preview"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ],
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": [
      "eslint:recommended",
      "plugin:vue/vue3-essential"
    ],
    "rules": {},
    "root": true
  },
  "dependencies": {
    "axios": "^1.2.1",
    "d3": "^7.7.0",
    "leaflet": "^1.9.3",
    "leaflet-geonames": "^0.4.9",
    "leaflet.markercluster": "^1.5.3",
    "timelines-chart": "^2.11.8",
    "unisort": "^1.1.0",
    "uuid": "^8.3.2",
    "vis-timeline": "^7.7.0",
    "vue": "^3.2.45",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@tailwindcss/forms": "^0.5.3",
    "@types/leaflet": "^1.9.0",
    "@types/leaflet.markercluster": "^1.5.1",
    "@types/uuid": "^9.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.29.0",
    "eslint-plugin-vue": "^9.8.0",
    "gh-pages": "^4.0.0",
    "postcss": "^8.4.20",
    "prettier": "^2.8.1",
    "prettier-plugin-tailwindcss": "^0.2.1",
    "stylelint": "^14.16.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "tailwindcss": "^3.2.4",
    "vite": "^4.0.1"
  }
}
